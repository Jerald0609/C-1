using System;

class Program
{
    static void Main()
    {
        string[] words = new string[5];
        for (int i = 0; i < 5; i++)
        {
            Console.Write($"Enter word {i + 1}: ");
            words[i] = Console.ReadLine();
            string rev = new string(words[i].ToCharArray().Reverse().ToArray());
            Console.WriteLine(words[i] == rev ? "Palindrome" : "Not a Palindrome");
        }
    }
}
