using System;

class Program
{
    static void Main()
    {
        int[] ages = new int[5];
        int teenagerCount = 0;

        for (int i = 0; i < ages.Length; i++)
        {
            while (true)
            {
                Console.Write($"Enter age for customer {i + 1}: ");
                string input = Console.ReadLine();

                if (int.TryParse(input, out int age))
                {
                    if (age >= 10 && age <= 99)
                    {
                        ages[i] = age;

                        if (age >= 13 && age <= 19)
                        {
                            teenagerCount++;
                        }

                        break; // exit the while loop once a valid age is entered
                    }
                    else
                    {
                        Console.WriteLine("Invalid age. Must be between 10 and 99.");
                    }
                }
                else
                {
                    Console.WriteLine("Invalid input. Please enter a number.");
                }
            }
        }

        Console.WriteLine("\nList of valid ages:");
        foreach (int age in ages)
        {
            Console.Write(age + " ");
        }

        Console.WriteLine($"\nNumber of teenagers: {teenagerCount}");
    }
}
